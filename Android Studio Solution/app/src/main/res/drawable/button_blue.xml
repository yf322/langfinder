<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="13dip" />
            <stroke android:width="1px" android:color="#337BC3" />
            <gradient android:angle="90" android:endColor="#297CCF" android:startColor="#3399FF" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="13dip" />
            <stroke android:width="1px" android:color="#337BC3" />
            <gradient android:angle="270" android:endColor="#297CCF" android:startColor="#33AAFF" />
        </shape>
    </item>
</selector>